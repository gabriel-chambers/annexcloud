{
	"name": "e25/realm",
	"type": "wordpress-plugin",
	"autoload": {
		"psr-4": {
			"E25m\\Realm\\": "lib/"
		}
	},
	"authors": [
		{
			"name": "Ravisha Heshan",
			"email": "ravisha.w@eight25media.com"
		}
	],
	"minimum-stability": "RC",
	"repositories": [
		{
			"type": "composer",
			"url": "https://repo.packagist.com/gandalf/"
		}
	],
	"require": {
		"php": "^7.3|^8.0",
		"composer/installers": "^1.8",
		"davidbarratt/custom-installer": "^1.1.0",
		"e25/r-blbslr-l4-anxcld1": "1.8059.001-rc2",
		"e25/r-cntcol-l1-anxcld1": "1.8059.001-rc2",
		"e25/r-tabslr-l2-anxcld1": "1.8059.001-rc2",
		"e25/r-spstbk-l1-anxcld1": "1.8059.001-rc1",
		"e25/r-tesslr-l1-rlm1": "1.8062.001-rc1",
		"e25/r-tesslr-l1-anxcld1": "1.8062.001-rc1",
		"e25/r-crdstk-l3-anxcld1": "1.8062.001-rc1",
		"e25/r-crdstk-l3-anxcld2": "1.8062.001-rc1",
		"e25/r-blbstk-l3-anxcld1": "1.8062000.001",
		"e25/r-crlcol-l1f-asn2": "1.8062000.001",
		"e25/r-crdstk-l4-anxcld1": "1.8062000.001",
		"e25/r-crdstk-l4-anxcld2": "1.8062000.001",
		"e25/r-cntcol-l1-anxcld2": "1.8062000.001",
		"e25/r-imgcnt-l1-anxcld1": "1.8062000.001",
		"e25/r-crdstk-l3-anxcld3": "1.8062000.001",
		"e25/r-tesimg-l2-anxcld1": "1.8062000.001"
	},
	"config": {
		"allow-plugins": {
			"composer/installers": true,
			"davidbarratt/custom-installer": true
		}
	},
	"extra": {
		"custom-installer": {
			"src/sections/{$name}": [
				"type:e25-realm-section"
			],
			"src/common-components/{$name}": [
				"type:e25-realm-base-section"
			],
			"./../berg/src/block/{$name}": [
				"type:wordpress-bergblock"
			]
		},
		"realm": {
			"common_folder_path": "src/common-components",
			"theme_folder_path": "../../themes/berg-theme-child",
			"exclude_theme_overrides": {}
		}
	},
	"scripts": {
		"post-package-install": [
			"E25m\\Realm\\Composer\\Installer::postPackageInstall"
		],
		"post-package-update": [
			"E25m\\Realm\\Composer\\Installer::postPackageUpdate"
		],
		"pre-package-uninstall": [
			"E25m\\Realm\\Composer\\Uninstaller::prePackageUninstall"
		],
		"archive-package": [
			"E25m\\Realm\\Composer\\ComposerUtils::archivePackage"
		],
		"setup": [
			"E25m\\Realm\\Composer\\SetupRealm::linkFilesComposer"
		],
		"post-install-cmd": [
			"@setup"
		],
		"post-update-cmd": [
			"@setup"
		]
	},
	"scripts-descriptions": {
		"archive-package": "Generate a ZIP file of installed Realm section package."
	},
	"config": {
		"allow-plugins": {
			"composer/installers": true,
			"davidbarratt/custom-installer": true
		}
	}
}
